            <g clip-path="url(#clip0_29_838)">
            <path d="M25.5051 11.3917C25.5037 11.3905 25.5037 11.3905 25.5051 11.3917L18.9219 5.33521C18.316 4.77777 17.3326 4.77777 16.7266 5.33521C16.1207 5.89266 16.1207 6.79743 16.7266 7.35487L20.6604 10.9739L2.46267 10.9727C1.60552 10.9727 0.910925 11.6117 0.910924 12.4003C0.912268 12.7946 1.08558 13.1518 1.36637 13.4101C1.64716 13.6685 2.03544 13.8279 2.46267 13.8279L20.6618 13.8279L16.728 17.447C16.1221 18.0044 16.1221 18.9092 16.728 19.4667C17.3339 20.0241 18.3173 20.0241 18.9233 19.4667L26.6027 12.4016L25.5051 11.3917Z" fill="#FF6B00"/>

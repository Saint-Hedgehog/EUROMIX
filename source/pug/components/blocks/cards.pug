        imgUrl: "content/catalog/accessories",
        altText: "Screw-conveyors-for-cement",
        href: "#"
      },
      {
        title: "Ленточные транспортеры",
        image: "belt-conveyors",
        imgUrl: "content/catalog/accessories",
        altText: "Belt-conveyors",
        href: "#"
      },
      {
        title: "Станции загрузки инертных материалов",
        image: "loading-stations-for-inert-materials",
        imgUrl: "content/catalog/accessories",
